{
  "name": "@repo/shared",
  "version": "0.0.0",
  "private": true,
  "types": "./dist/types.d.ts",
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./enums": {
      "types": "./dist/enums.d.ts",
      "default": "./dist/enums.js"
    },
    "./dto/*": {
      "types": "./dist/dto/*.d.ts",
      "default": "./dist/dto/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check-types": "tsc -p tsconfig.json --noEmit"
  }
}
