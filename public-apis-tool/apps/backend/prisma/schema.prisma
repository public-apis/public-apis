generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id Int @id @default(autoincrement())
  githubId Int @unique
  login String
  avatar String?
  githubToken String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
